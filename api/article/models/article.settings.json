      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "content": {
      "type": "richtext"
    },
    "author": {
      "type": "string"
    },
    "datePublished": {
      "type": "date"
    },
    "dateEdited": {
      "type": "date"
    },
    "category": {
      "model": "category"
    }
  }
